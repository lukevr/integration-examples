{
  "name": "zaka-integration-example",
  "version": "0.0.1",
  "description": "integration example for zaka",
  "requires": true,
  "lockfileVersion": 1,
  "scripts": {
    "start": "./node_modules/.bin/ts-node src/index.ts",
    "start:cluster": "./node_modules/.bin/ts-node src/cluster.ts",
    "build": "npm run clean && tsc && tslint ./src/**/*.ts && npm run swagger",
    "clean": "rimraf dist",
    "lint": "tslint ./src/**/*.ts ./test/**/*.ts",
    "lint:fix": "tslint --fix ./src/**/*.ts ./test/**/*.ts -t verbose",
    "pretest": "cross-env NODE_ENV=test npm run build && npm run lint",
    "test": "cross-env NODE_ENV=test mocha ./test/**/*.spec.ts --exit --delay",
    "test-sc": "cross-env NODE_ENV=test mocha ./test/**/*.spec.ts --exit --delay -g service-config",
    "test:coverage": "nyc npm run test",
    "swagger": "swaggerGen -c ./swagger.config.json",
    "tsc": "tsc"
  },
  "dependencies": {
    "@types/mongoose": "^5.7.30",
    "@types/mustache": "^4.0.1",
    "axios": "^0.19.2",
    "bcrypt": "^5.0.0",
    "cors": "2.8.5",
    "dotenv": "8.0.0",
    "hjson": "^3.2.1",
    "mongoose": "^5.9.22",
    "morgan": "^1.10.0",
    "mustache": "^4.0.1",
    "typescript-ioc": "^1.2.5",
    "typescript-rest": "^2.2.2",
    "uuid": "^8.2.0",
    "uuidv4": "^6.1.1"
  },
  "disabled-dependencies": {},
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/chai": "4.1.7",
    "@types/cors": "2.8.5",
    "@types/dotenv": "6.1.1",
    "@types/express": "4.16.1",
    "@types/hjson": "^2.4.1",
    "@types/morgan": "^1.9.0",
    "@types/node": "^12.0.10",
    "@types/uuid": "^8.0.0",
    "chai": "4.2.0",
    "cross-env": "5.2.0",
    "mocha": "6.1.4",
    "node-deb": "^0.10.7",
    "nyc": "14.1.1",
    "rimraf": "2.6.3",
    "source-map-support": "0.5.12",
    "ts-node": "8.2.0",
    "tslint": "5.17.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "^3.7.2",
    "typescript-rest-swagger": "^0.1.0"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "report-dir": "./reports/coverage",
    "sourceMap": true,
    "instrument": true
  },
  "node_deb": {
    "package_version": "0.0.1",
    "entrypoints": {
      "daemon": "runUbichProxy.sh"
    },
    "templates": {
      "executable": "deb-templates/executable"
    }
  }
}
